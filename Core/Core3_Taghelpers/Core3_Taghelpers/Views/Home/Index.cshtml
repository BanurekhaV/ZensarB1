@model IEnumerable<Product>

<table  class="table table-bordered">
    <thead class="bg-dark text-white" >
        <tr>
            <th>Name</th>
            <th>Price</th>
            <th>Quantity</th>
        </tr>
    </thead>
    <tbody>
        @foreach(var product in Model)
            {
        <tr>
            <td>@product.Name</td>
            <td>@product.Price</td>
            <td>@product.Quantity</td>
        </tr>
            }
    </tbody>
</table>

