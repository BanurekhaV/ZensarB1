@model IEnumerable<Day1MVCPrj.Models.Employee>
@{
    ViewBag.Title = "ListEmployeeDetails";
}

<h2>ListEmployeeDetails</h2>

<table class="table table-striped">
    <thead>
        <tr>
            <th>Emp ID</th>
            <th>Employee Name</th>
            <th>Employee Age</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.ID</td>
                <td>@item.Name</td>
                <td>@item.Age</td>
            </tr>
        }

    </tbody>
</table>